{
  "name": "@strawbees/code-ui",
  "version": "1.11.1",
  "scripts": {
    "dev": "node lib/scripts/dev.js",
    "build": "node lib/scripts/build.js",
    "build-serve": "node lib/scripts/buildServe.js",
    "build-targets": "node lib/scripts/buildTargets.js",
    "i18n:scratch-blocks:pull": "node lib/scripts/i18nPull.js",
    "i18n:zanata:pull": "zanata-cli pull --username $ZANATA_USER --key $ZANATA_KEY --create-skeletons --errors",
    "i18n:zanata:push:templates": "zanata-cli push --username $ZANATA_USER --key $ZANATA_KEY --errors",
    "i18n:zanata:push:translations": "zanata-cli push --username $ZANATA_USER --key $ZANATA_KEY --push-type trans --errors",
    "i18n:pull": "npm run i18n:zanata:pull && npm run i18n:scratch-blocks:pull",
    "prepublishOnly": "npm run build-targets",
    "lint": "eslint --fix ./*.js ./src/**.js ./src/**/**.js ./src/**/**/**.js ./src/**/**/**/**.js ./src/**/**/**/**/**.js ./pages/**.js"
  },
  "bin": {
    "strawbees-code-ui-build": "./lib/scripts/build.js"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@strawbees/quirkbot-chrome-app": "^0.3.4",
    "@svgr/webpack": "^5.4.0",
    "file-saver": "^2.0.2",
    "informed": "1.10.12",
    "lz-string": "^1.4.4",
    "next": "9.3.4",
    "next-redux-wrapper": "^6.0.2",
    "node-fetch": "^2.6.1",
    "platform": "^1.3.6",
    "pofile": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-ace": "^9.1.4",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.4.3",
    "react-dropzone": "^11.2.0",
    "react-ga": "^3.1.2",
    "react-load-script": "^0.0.6",
    "react-markdown": "^4.3.1",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "scratch-blocks": "0.1.0-prerelease.20200930031639",
    "serve-handler": "^6.1.3",
    "tinycolor2": "^1.4.2",
    "workbox-build": "^5.1.4",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
